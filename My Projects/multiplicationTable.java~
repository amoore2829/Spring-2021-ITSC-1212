class multiplicationTable{
  public static void main(String args[]){


int [][] my2DIntArray = new int[10][10];

for (int i = 1; i <= 10; i++) {
  for (int j = 1; j <= 10; j++) {
    my2DIntArray[i-1][j-1] = i*j;
    System.out.print(my2DIntArray[i-1][j-1] + ", ");
  }
  System.out.println();
}

System.out.println("2 times 4 is " + my2DIntArray[2][4]);