int num = 7;
System.out.println("Song titles with numbers include: ");
switch (num) {
 case 12:
  System.out.println("12 days of Christmas");
  break;
 case 99:
  System.out.println("99 bottles of beer on the wall");
  System.out.println("99 Luftballons");
  break;
 case 7:
  System.out.println("7 Chinese Brothers");
  break;
 case 50:
  System.out.println("50 Mission Cap");
  break;
 default:
  System.out.println("If I had a million dollars");
}