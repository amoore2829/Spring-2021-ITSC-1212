import java.util.Scanner;                       // Include this line to import the Scanner utility

public class Lab6PartC
{
  public static void main(String [] args) 
  {
    // Part 1
    Scanner keyboard = new Scanner(System.in);  // Include these two
    int x = keyboard.nextInt();                 // lines for integers
    System.out.println(x);
    System.out.println(x + 3);
    
    // Part 2
    String str1 = keyboard.next();              // Include these two lines for strings and 
    String str2 = keyboard.nextLine();          // remember to combine the strings before using them (see below)
    System.out.println("You entered \""+str1+str2+"\"");
  }
}